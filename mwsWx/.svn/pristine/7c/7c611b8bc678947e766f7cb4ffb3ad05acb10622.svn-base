<!--取消付款-->
<div class="pop-overlay" style="display:none"></div>
<div class="popup pop-orderCancer changeContact" style="display: none">
  <div class="pop-content">
    <div class="ct-cancel">
    	<h2 class="pop-title">取消订单</h2>
    	<div class="pop-tips">
    		<p>确定取消订单吗？</p>
    	</div>
    </div>
    <div class="ct-sureRec" style="display:none">
    	<h2 class="pop-title">确认收货</h2>
    	<div class="pop-tips">
    		<p>确定收货吗？请确保收到商品后再确认收货。</p>
    	</div>
    </div>
    
    <div class="btn-box clearfix">
    	<a href="javascript:;" class="change-cancel">放弃</a>
    	<a href="javascript:;" class="change-sure active">确定</a>
    </div>
  
  </div>
</div>

<script src="../js/jquery-1.7.1.min.js"></script>
<script type="text/javascript">
	$(document).ready( function(){

		 //放弃付款
	    $('.sun-cancelPay').click(function(){
	    	$('body').css('overflow','hidden');
	    	$('.pop-overlay').show().animate({opacity:0.7},450);
	    	$('.changeContact').show();
	    	$('.ct-cancel').show();
	    	$(".ct-sureRec").hide();
	    });
	    //确认收货
	      $('.btn-sureReceive').click(function(){
	      	$('body').css('overflow','hidden');
	    	$('.pop-overlay').show().animate({opacity:0.7},450);
	    	$('.changeContact').show();
	    	$('.ct-cancel').hide();
	    	$(".ct-sureRec").show();
	    });

	    //关闭弹窗
	    $('.change-cancel').click(function(){
	    	$('.pop-overlay').animate({opacity:0},500,function(){
	    		$('.pop-overlay').hide();
	    	});
	    	$('.changeContact').hide();
	    	$('body').css('overflow','visible');
	    });

	    //按钮
	    $('.btn-box').on('click','a',function(){
	    	$(this).addClass('active');
	    	$(this).siblings().removeClass('active');
	    });

	    //头部添加样式
	    $(".sun-bale-tab1 ul").on('click','span',function(){
	    	$(this).addClass('active');
	    	$(this).parent().siblings().find('span').removeClass('active');

	    });



	})
</script>
