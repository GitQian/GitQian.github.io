  <!--选择规格弹窗-->
<!-- <div class="pop-overlay" style="display:none"></div> -->
<div class="popup" id="pop-chooseType"  style="display:none">  
  <div class="pop-content">
    <!--商品基本信息-->
    <div class="imgWrap chooseTypeBox clearfix">
      <div class="imgWrap-itemPic">
        <img src="../images/pc_toux.png"></div>
      <div class="imgWrap-itemInfo">
        <p class="itemInfo-price">￥2100~12444.00</p>
        <p class="itemInfo-spare">
          库存: <em>100</em>
        </p>
        <p class="itemInfo-type" >
          <span>4G版本</span>
          <span>套餐2</span>
        </p>
      </div>
      <a href="javascript:;" class="typeClose-btn">
        <img src="../images/index_09.gif" alt=""></a>
    </div>
    <!--规格栏目-->
    <div class="typeColumn">
      <!--选择车型-->
      <div class="firstTypeWrap chooseTypeBox">
        <h2>选择车型</h2>
        <div class="typeBox clearfix">
            <!--选择车型下拉单-->
          <ul class=" clearfix">
            <li class="carSel-brand carSel-li">
              <select>
                <option>请选择汽车品牌</option>
                <option>湖北省</option>
                <option>湖南省</option>
              </select>
            </li>
            <li class="carSel-series carSel-li">
              <select>
                <option>请选择车系</option>
                <option>所有城市</option>
                <option>深圳市</option>
              </select>
            </li>
            <li class="carSel-year carSel-li">
              <select>
                <option>请选择年款</option>
                <option>克孜勒苏柯尔克孜自治州</option>
                <option>深圳市</option>
              </select>
            </li>
          </ul>
        </div>
      </div>
      <!--规格分类1-->    
      <div class="firstTypeWrap chooseTypeBox">
        <h2>规格分类1</h2>
        <div class="typeBox clearfix">
          <a href="javascript:;">4G版</a>
          <a href="javascript:;">WIFI版</a>
        </div>
      </div>
      规格分类2   
      <div class="secondTypeWrap chooseTypeBox">
        <h2>规格分类2</h2>
        <div class="typeBox clearfix">
          <a href="javascript:;" class="selected">套餐1</a>
          <a href="javascript:;">套餐2</a>
          <a href="javascript:;">套餐3</a>
          <a href="javascript:;">套餐4</a>
        </div>
      </div>
      <!--安装类型-->   
      <div class="installTypeWrap chooseTypeBox">
        <h2>安装类型</h2>
        <div class="typeBox clearfix">
          <a href="javascript:;" class="selected">到店安装</a>
          <a href="javascript:;">无需安装</a>
        </div>
      </div>
      <!--服务类型-->   
      <!-- <div class="serviceTypeWrap chooseTypeBox">
        <h2>服务类型</h2>
        <div class="typeBox clearfix">
          <a href="javascript:;" class="selected">安装1</a>
          <a href="javascript:;">安装2</a>
        </div>
      </div> -->
      <!--安装门店-->   
      <!-- <div class="installStoreWrap chooseTypeBox">
        <h2>安装门店</h2>
        <a href="javascript:;" class="serviceLink">美赛达安装点（深圳市南山区清华信息港2号）</a>
      </div> -->
      <!--商品数量的增减-->    
      <div class="numTypeWrap chooseTypeBox">
        <h2>数量</h2>
        <div class="num-box">
          <a href="javascript:;" class="choose-reduce noOperate per-btn">-</a>
          <a href="javascript:;" class="choose-num per-btn">1</a>
          <a href="javascript:;" class="choose-add per-btn">+</a>
        </div>
      </div>

    </div>
    
    <!--加入购物车或者购买-->
    <div class="btn-box chooseTypeBtn clearfix" >
      <a href="javascript:;" class="chooseBtn active">加入购物车</a>
      <a href="javascript:;" class="chooseBtn" >立即购买</a>
    </div>
    <!--确认订单-->
    <div class="btn-box chooseTypeSure" style="display: none">
      <a href="javascript:;" class="btn-center active">确定</a>
    </div>
  </div>
</div>