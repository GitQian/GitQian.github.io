<script type="text/javascript">
   var h=$(window).height();
       $('.popup').css('top',h);
	$(function(){

		$(".sun-li li").click( function(){
			$(this).addClass("crunte").siblings().removeClass("crunte");
			$(".sun-content div").hide().eq($('.sun-li li').index(this)).show();
		});

		var minNum=1;
		var maxNum=9999;


		//产品数量增加
	    $("#pop-chooseType").on('click','.choose-add',function(event){
	     var baseNum=$(this).siblings('.choose-num').val();
	     if(++baseNum>maxNum){
	     	baseNum=maxNum;
	     }
	     $(this).siblings('.choose-num').val(baseNum);
	     $(this).siblings('.choose-reduce').removeClass('noOperate');
	    });

		//产品数量减少
	    $("#pop-chooseType").on('click','.choose-reduce',function(event){
	     var baseNum=$(this).siblings('.choose-num').val();
	      baseNum==2?$(this).addClass("noOperate"):'';
	     if(--baseNum<minNum){  
	        baseNum=minNum;
	     }
	       $(this).siblings('.choose-num').val(baseNum);
	    });

