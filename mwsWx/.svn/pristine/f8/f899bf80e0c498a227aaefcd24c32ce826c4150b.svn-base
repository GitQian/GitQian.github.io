	});

function openPop(){
	$('.pop-overlay').show().animate({opacity:0.7},450);
	$('.popup').animate({top:0},500);
	$('.popup').show();
	$(".chooseTypeBtn").hide();
	$(".chooseTypeSure").show();
	$("body").css('overflow','hidden');
}

function closePop(){
	$('.pop-overlay').animate({opacity:0},500,function(){
		$('.pop-overlay').hide();
	});
