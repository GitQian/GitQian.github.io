@charset "UTF-8";

html,body,h1,h2,h3,h4,h5,h6,p,br,form,input,button,textarea,select,fieldset,legend,blockquote,ul,ol,li,dl,dt,dd,pre,th,td,div{margin:0;padding:0;}
html{font-size: 62.5%;}
body{font-family: 'Microsoft YaHei';color:#222;background: #f5f5f5;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
a{color: #333;text-decoration: none;cursor: pointer;outline:none;}
a:hover{text-decoration:none;outline:none;}
a:active {star:expression(this.onFocus=this.blur());}
a:focus{outline:none;-moz-outline:none;}
textarea,select,input,button{vertical-align:middle;font:12px Tahoma;resize: none;}
textarea{padding: 5px;border-radius: 5px;border:1px solid #E0E0E0;}
button{background:none;border:none;cursor:pointer}
address,cite,code,em,var,acronym{font-style:normal;}
pre{font-size:14px;text-align:center;white-space:pre-wrap;}
table{border-collapse:collapse;}
div,li,dt,dd,p,pre,td,th{word-wrap:break-word;word-break:break-all;}
ol,ul{list-style:none;}
li{list-style-position:outside;}
input{outline:none;}
label,input[type="radio"]{cursor: pointer;}
img {border: 0;max-width: 100%;height: auto; }
fl{ float: left;}
fl{ float: right;}

/*clear float*/
.clearfix:before,.clearfix:after{content:".";display:block;height:0;visibility:hidden;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.clearall{overflow:hidden;_zoom:1;}
.clear{clear:both;font-size:0;height:0;line-height:0;}

/*公共*/
.mt15{margin-top: 15px;}

.wxPage .pagebox{background: #fff;width: 100%;border-bottom: 1px solid #e1e1e1;}
.wxPage .pagebox .page-box{padding: 0 2%;margin: 0 auto;}

/*线下门店*/
.ofPage .of-title h3{font-size: 2.4rem;color:#4A4A4A;line-height: 2.5;display:inline-block;/* height: 100%; *//* vertical-align: middle; */}
.ofPage .of-title .of-btn{display:inline-block;float: right;/* width: 30%; */margin: 10px 0;}
.ofPage .of-title .of-btn p{border:1px solid #CBCBCB;background:rgba(241, 241, 241, 0.6);border-radius:4px;padding: 5px 14px;/* width: 100%; */text-align: center;}
.ofPage .of-title .of-btn a{font-size:24px;color:#2196F3;line-height: 1.5;}
.ofPage .of-info .of-info-tx{display:inline-block;width: 25%;max-width:195px;/* float: left; *//* line-height: 2; */margin-top: 2%;}
.ofPage .of-info .of-info-detail{display:inline-block;vertical-align:top;width: 69%;margin-left: 10px;}
.ofPage .of-info .of-info-detail ul li{font-size: 1.8rem;color:#222;line-height: 1.714;}
.ofPage .of-info .of-info-detail ul li+li{color:#666;}



/*个人中心*/
.pcPage .pc-user{vertical-align: middle;height: 100%;position: relative;}
.pcPage .pc-user .pc-user-tx{display:inline-block;width:25%;max-width: 192px;padding: 10px 0;}
.pcPage .pc-user .pc-user-info{display:inline-block;vertical-align: top;height: 100%;padding-left: 25px;padding-top: 5%;}
.pcPage .pc-user .pc-user-info h2{font-size: 24px;color:#222;line-height:1.5;}
.pcPage .pc-user .pc-user-info h3{font-size: 21px;color:#666;line-height:1.714;}
.pcPage .pc-user .pc-jiant{position: absolute;right: 5%;top: 30%;max-width: 17px;}
.pcPage .pc-num{text-align: center;border-bottom: 1px solid #DADADA;}
.pcPage .pc-num ul{width: 80%;margin: 0 auto;}
.pcPage .pc-num ul li{float:left;width: 33%;padding:15px 0 10px;}
.pcPage .pc-num h3{font-size: 30px;color: #222;margin-bottom: 10px;font-weight: bold;}
.pcPage .pc-num p{font-size:18px;color: #666;}
.pcPage .pc-gn .pc-gn-box{ position: relative; height: 65px;line-height:65px;padding-left: 5%;}

.pcPage .pc-gn .img-gn{background: url(../images/pc_bg.png) no-repeat;background-size:50% ;height: 30px;width: 53px;display: inline-block;vertical-align: middle;float: left;margin-top: 20px;}
.pcPage .pc-gn .gn-order{background-position: 0px 0px;}
.pcPage .pc-gn .gn-buyCar{background-position: 0px -30px;}
.pcPage .pc-gn .gn-mydd{background-position: 0px -60px;}
.pcPage .pc-gn .gn-jy{background-position: 0px -90px;}
.pcPage .pc-gn .gn-xx{background-position: 0px -120px;}
.pcPage .pc-gn .gn-sc{background-position: 0px -150px;}
.pcPage .pc-gn .pc-gn-name{height: 100%;margin-top: 0;padding-left: 17px;border-bottom: 1px solid #e1e1e1;margin-left:53px;}
.pcPage .pagebox-nob .pc-gn .pc-gn-name{border-bottom: none}
.pcPage .pc-gn span,.pcPage .pc-edit-toux span{font-size:21px;color: #222;line-height:1.714}
.pcPage .pc-gn img{position: absolute;right: 3%; top: 26%;max-width: 17px; }


/*个人中心-编辑资料*/
.pagebox.eidtPagebox{background: #f5f5f5;}
.pagebox.eidtPagebox .pc-edit-info{background: #fff;}
.eidtPagebox>h2{line-height: 4rem;font-size: 1.7rem;padding-left:8px;}
.pcPage .pc-edit-info dl{line-height:2.2rem;display: flex;display: box;display:-webkit-flex;flex-flow:row nowrap;padding: 15px 8px;position: relative;text-align:right;font-size:1.8rem;color: #222;border-bottom: 1px solid #e1e1e1;}
.pcPage .pc-edit-info dl.dl-last{border-bottom: none}
.pcPage .pc-edit-info dl>dt{text-align: left;flex:2;}
.pcPage .pc-edit-info dl>dd{text-align: right;flex:3;}
.pcPage .pc-edit-toux .edit-name{line-height: 7rem;}
.pcPage .edit-img-toux span.face-edit{ position: relative;cursor: pointer;width:7rem;height: 7rem;overflow: hidden;float:right;*display: inline;*zoom: 1;display: block;background: url(../images/face.png) no-repeat;background-size: 100%; }
.pcPage .edit-img-toux .face-edit>input {width: 100%;height: 100%;position: absolute;font-size: 100px;right: 0;top: 0;opacity: 0;filter: alpha(opacity=0);cursor: pointer;}
.pcPage .pc-edit-info dl>dd .edit-cls{display: block;width:80%;padding-right: 20%;}
/**编辑资料-车牌号相机**/
.pcPage .pc-edit-info dl>dd.p-camera{position: relative;}
.pcPage .pc-edit-info dl>dd.p-camera input{width: 80%;padding-right: 20%;z-index: 999;}
.pcPage .pc-edit-info dl>dd.p-camera .edit-icon{position: absolute;top:0;right: 0;z-index: 998;width: 20%;}
.pcPage .pc-edit-info dl>dd.p-camera .edit-icon img{width: 100%;max-width: 2.5rem;vertical-align: -10px;}
.pcPage .pc-edit-info dl>dd .icon-lead{background: url(../images/icon_zjt.png) 100% 50% no-repeat;background-size: 5%;}

.pcPage .pc-edit-info input[type="text"],input[type="date"],select{width: 100%;height:100%;border:none;text-align:right;font-size:1.8rem;}
.pcPage .pc-edit-info input::-webkit-input-placeholder {text-align: right;}
.pcPage .pc-edit-info input::-moz-placeholder {text-align: right;} /* firefox 19+ */
.pcPage .pc-edit-info input::-ms-input-placeholder {text-align: right;} /* ie */
.pcPage .pc-edit-info input:-moz-placeholder {text-align: right;}
/*编辑资料-下拉单*/
.ui-select {text-align: right;position: relative;padding-right: 20%; }  
.ui-select select {position: absolute;left: 0px;top:-1.8rem;width: 100%;height: 3em;opacity: 0;} 

.wxPage .page-btn p{text-align: center;}
.wxPage .page-btn p a{display: block;width:90%;line-height:50px;background: #FF5E3E;border-radius: 6px;margin: 50px auto 30px;color:#fff;max-width: 380px;font-size:24px;}


/**线下门店**/
.storeInfo{width: 100%;margin-top: 10px;box-sizing: border-box;position: relative;}
.storeInfo>div{padding: 15px 3%;width: 94%;float: left;background: #fff;}
.storeInfo>div .search-img{float: left;}
.storeInfo>div .search-img img{max-width: 110px;max-height: 110px;width:100%;height: 100%;}
.storeInfo>div .search-div{float: left;width: 55%;margin-left: 15px;}
.storeInfo>div .search-div p{font-size: 1.65rem;line-height: 24px;color: #666;padding-bottom:0.8rem;padding-right: 5px;}
.storeInfo>div .search-div p.store-name{color:#000;font-size: 1.8rem;}
.storeInfo>div i{display: block;position: absolute;}
.storeInfo>div i.icon-ps{width:20px;height: 30px ;right: 10px;top:10px;background: url(../images/sunicon_03.png) 0 0 no-repeat;background-size: 100%;}
.storeInfo>div i.icon-tel{width:40px;height: 40px ;right: 5px;top:50%;margin-top:55px;background: url(../images/icon-tel.png) 0 0 no-repeat;background-size: 100%;}

.sun-page-bottom{position: fixed;bottom:0;width: 100%;height: 150px;line-height: 150px;}

