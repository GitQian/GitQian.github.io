@media screen and (min-width: 800px){
	.pcPage .pc-edit-toux .edit-name{padding: 10% 8px 0;}
	.pcPage .pc-edit-info dl>dd .icon-lead{background-size: 2%;}
}

@media screen and (min-width:480px) and(max-width:800px){
	body{background: red;}
	.pcPage .pc-edit-info dl>dd .icon-lead{background-size: 3%;}
}

@media screen and (max-width:375px) and (min-width:320px){
	.storeInfo>div .search-img img{max-width: 80px;max-height: 80px;width:100%;height: 100%;}
	.storeInfo>div .search-div p{font-size:1rem;padding-bottom: 0.3rem;line-height: 20px; }

}




