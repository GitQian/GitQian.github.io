@charset "UTF-8";

html,body,h1,h2,h3,h4,h5,h6,p,br,form,input,button,textarea,select,fieldset,legend,blockquote,ul,ol,li,dl,dt,dd,pre,th,td,div{margin:0;padding:0;}
html{font-size: 62.5%;}
body{font-family: 'Microsoft YaHei';color:#222;background: #f5f5f5;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
a{color: #333;text-decoration: none;cursor: pointer;outline:none;}
a:hover{text-decoration:none;outline:none;}
a:active {star:expression(this.onFocus=this.blur());}
a:focus{outline:none;-moz-outline:none;}
textarea,select,input,button{vertical-align:middle;font:12px Tahoma;resize: none;}
textarea{padding: 5px;border-radius: 5px;border:1px solid #E0E0E0;}
button{background:none;border:none;cursor:pointer}
address,cite,code,em,var,acronym{font-style:normal;}
pre{font-size:14px;text-align:center;white-space:pre-wrap;}
table{border-collapse:collapse;}
div,li,dt,dd,p,pre,td,th{word-wrap:break-word;word-break:break-all;}
ol,ul{list-style:none;}
li{list-style-position:outside;}
input{outline:none;}
label,input[type="radio"]{cursor: pointer;}
img {border: 0;max-width: 100%;height: auto; }
fl{ float: left;}
fl{ float: right;}

/*clear float*/
.clearfix:before,.clearfix:after{content:".";display:block;height:0;visibility:hidden;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.clearall{overflow:hidden;_zoom:1;}
.clear{clear:both;font-size:0;height:0;line-height:0;}

/*公共*/
.mt15{margin-top: 15px;}

.wxPage .pagebox{background: #fff;width: 100%;}
.wxPage .pagebox .page-box{padding: 0 2%;margin: 0 auto;}

/*线下门店*/
.ofPage .of-title h3{font-size: 2.4rem;color:#4A4A4A;line-height: 2.5;display:inline-block;/* height: 100%; *//* vertical-align: middle; */}
.ofPage .of-title .of-btn{display:inline-block;float: right;/* width: 30%; */margin: 10px 0;}
.ofPage .of-title .of-btn p{border:1px solid #CBCBCB;background:rgba(241, 241, 241, 0.6);border-radius:4px;padding: 5px 14px;/* width: 100%; */text-align: center;}
.ofPage .of-title .of-btn a{font-size:24px;color:#2196F3;line-height: 1.5;}
.ofPage .of-info .of-info-tx{display:inline-block;width: 25%;max-width:195px;/* float: left; *//* line-height: 2; */margin-top: 2%;}
.ofPage .of-info .of-info-detail{display:inline-block;vertical-align:top;width: 69%;margin-left: 10px;}
.ofPage .of-info .of-info-detail ul li{font-size: 1.8rem;color:#222;line-height: 1.714;}
.ofPage .of-info .of-info-detail ul li+li{color:#666;}



/*个人中心*/
.pcPage .pc-user{vertical-align: middle;height: 100%;position: relative;}
.pcPage .pc-user .pc-user-tx{display:inline-block;width:30%;max-width: 192px;padding: 15px 0;}
.pcPage .pc-user .pc-user-info{display:inline-block;vertical-align: top;height: 100%;padding-left: 25px;padding-top: 5%;}
.pcPage .pc-user .pc-user-info h2{font-size: 24px;color:#222;line-height:1.5;}
.pcPage .pc-user .pc-user-info h3{font-size: 21px;color:#666;line-height:1.714;}
.pcPage .pc-user .pc-jiant{position: absolute;right: 5%;top: 30%;}
.pcPage .pc-num{text-align: center;border-bottom: 1px solid #DADADA;}
.pcPage .pc-num ul{width: 80%;margin: 0 auto;}
.pcPage .pc-num ul li{float:left;width: 33%;padding: 25px 0 20px;}
.pcPage .pc-num h3{font-size: 30px;color: #222;margin-bottom: 20px;font-weight: bold;}
.pcPage .pc-num p{font-size: 21px;color: #666;}
.pcPage .pc-gn .pc-gn-box{ position: relative; height: 120px;line-height: 120px;padding-left: 5%;}
.pcPage .pc-gn .img-gn{background: url(../images/pc_gn.png) no-repeat;height: 78px;width: 78px;display: inline-block;vertical-align: middle;float: left;margin-top: 20px;}
.pcPage .pc-gn .gn-order{background-position: -33px -40px;}
.pcPage .pc-gn .gn-buyCar{background-position: -152px -40px;}
.pcPage .pc-gn .gn-mydd{background-position: -280px -40px;}
.pcPage .pc-gn .gn-jy{background-position: -408px -40px;}
.pcPage .pc-gn .gn-xx{background-position: -33px -177px;}
.pcPage .pc-gn .gn-sc{background-position: -152px -177px;}
.pcPage .pc-gn .pc-gn-name{height: 100%;margin-top: 0;padding-left: 17px;border-bottom: 1px solid #DADADA;margin-left: 78px;}
.pcPage .pc-gn span,.pcPage .pc-edit-toux span{font-size:21px;color: #222;line-height:1.714}
.pcPage .pc-gn img{position: absolute;right: 3%; top: 26%; }
/*个人中心-编辑资料*/
.pcPage .pc-edit-toux{border: 1px solid #E7E7E7;}
.pcPage .pc-edit-toux .edit-name{display: inline-block;padding: 13% 8px 0;}
.pcPage .edit-img-toux{width: 30%;display: inline-block;padding: 10px 8px;float: right;text-align: right;}
.pcPage .pc-edit-info table{width: 100%;}
.pcPage .pc-edit-info table tr{border-bottom: 1px solid #F3F3F3;}
.pcPage .pc-edit-info td{line-height: 40px;padding: 15px 8px;position: relative;text-align:right;font-size: 21px;color: #222;}
.pcPage .pc-edit-info tr td:first-child{width: 40%;text-align:left;}
.pcPage .pc-edit-info img{width: 20px;position: absolute;top: 18px;right: 10px;}
.pcPage .pc-edit-info span{color:#999;font-size: 21px; padding-right: 25px;}
.pcPage .pc-edit-info input[type="text"]{width: 80%;height:100%;padding: 0 5px;border:none;text-align:right;font-size: 21px;}
.pcPage .pc-edit-info input::-webkit-input-placeholder {text-align: right;}
.pcPage .pc-edit-info input::-moz-placeholder {text-align: right;} /* firefox 19+ */
.pcPage .pc-edit-info input::-ms-input-placeholder {text-align: right;} /* ie */
.pcPage .pc-edit-info input:-moz-placeholder {text-align: right;}

.wxPage .page-btn p{text-align: center;}
.wxPage .page-btn p a{display: block;width:90%;line-height:50px;background: #FF5E3E;border-radius: 6px;margin: 50px auto 30px;color:#fff;max-width: 380px;font-size:24px;}


@media screen and (min-width: 800px){.pcPage .pc-edit-toux .edit-name{padding: 10% 8px 0;}}



