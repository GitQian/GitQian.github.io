/*我的预约*/
.sun-res-det{background:#fff;padding: 10px 0;border-bottom: 1px solid #f0f0f0;}
.stepWrap{width: 100%;padding:20px 0;display:box;display: flex;display: -webkit-flex;display: -webkit-box;align-items:center;flex-flow:row nowrap;}
.stepWrap .step{flex:1;-webkit-box-flex:1;-webkit-flex:1;text-align: center;position: relative;}
.stepWrap .step .stepDot{margin:0 auto 15px;width:22px;height:22px;border:2px solid #fff;background: #ccc;border-radius:50%;position: relative;z-index: 2;}
.stepWrap .step .step-line{background:#ccc;width: 50%;height:3px;position: absolute;top:12px;z-index: 1;}
.stepWrap .step.on .stepDot,.step.on .step-line.on{background: #ff5e3e;}
.stepWrap .step .step-line.lineLeft{left: 0;}
.stepWrap .step .step-line.lineRight{right: 0;}

.sun-res-dets{width:94%;padding:0 3%;}
.sun-res-dets ul li{width: 100%;font-size:1.2rem;line-height: 30px;display:flex;display: -webkit-box;display: -webkit-flex;}
.sun-res-dets ul li span{color: #999999;display:inline-flex;}
.sun-res-dets ul li p{color: #222222;-webkit-box-flex:1;flex:1;}
.sun-res-a{width:80%;padding: 0 10%;margin: 0 auto 15px;}
.sun-res-a a{display: block;border: 1px #cbcbcb solid;border-radius: 5px 5px;font-size:1.2rem;color: #666;padding: 5px 0;text-align: center;margin-left: 10px;float: left;margin-top: 10px;width: 45%;}
.sun-res-a a.active{border-color: #ff5e3e;color:#ff5e3e;}
.sun-res-map{width: 94%;padding: 10px 3%;background: #fff;border-bottom: 1px #f0f0f0 solid;margin-top: 10px;display: flex;display: -webkit-box;display: -webkit-flex;}
.sun-res-img{width: 80px;height: 80px;}
.sun-res-img img{width: 100%;}
.sun-res-map1{-webkit-box-flex:1;flex:1;margin-left: 2%;font-size:1.2rem;line-height: 26px;}
.sun-res-map1 p{color: #666;font-size:1rem;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;word-break: break-all;}
.sun-rel-map2 a img{width:15px;height:100%;}

