@charset "UTF-8";
/***预约***/
.sun-order{width: 100%;height: 100%;}
.sun-order table{background: #fff;width: 100%;margin: 0 auto;box-sizing: border-box;}
.sun-order table tr td{font-size:1.2rem;color: #222222;height: 50px;line-height: 50px;width: 50%;border-bottom: 1px #e1e1e1 solid;padding: 0 3%;}
.sun-order table tr:nth-child(3),.sun-order table tr:nth-child(5),.sun-order table tr:nth-child(6){border-bottom: 10px #f5f5f5 solid;}
.sun-order table tr .sun-order-right{text-align: right;color: #a9a9a9;font:1.2rem Tahoma;}
.sun-order table tr .sun-order-right img{width: 10px;height: 20px;vertical-align: middle;padding-left: 10px;}
.sun-order table tr .sun-order-right input{background: #fff;border: 0;height: 45px;line-height: 45px;font-size:1.2rem;text-align: right;}
.sun-order table tr textarea{border: 0;width: 96%;height: auto;font-size:1.2rem;float: left;min-height: 70px;}
.sun-order table tr span{width: 3px;height: 18px;background: #ff5e3e;display: block;float: left;margin-top: 7px;}
.appointmentBox{margin-top:30px;}

/***微信关注***/
.sun-attention{width: 94%;height: 100%;padding:0 3% 20% 3%;background: #fff;}
.sun-attention p{width: 100%;font-size:1.2rem;color: #222222;}
.sun-attention-time{width: 100%;font-size:1rem;padding-top: 10px;}
.sun-attention-time b{color: #2d74bd;padding-left: 15px;font-weight: normal;}
.sun-attention-wei{width: 92%;border: 1px #ccc solid;margin-top: 20px;padding: 16px 4%;height: 120px;line-height: 120px;background: #f7f7f7;}
.sun-attention-wei img{float: left;vertical-align: middle;}
.sun-attention-wei span{float: left;font-size:1.2rem;color: #222222;padding-left: 15px;}
.sun-attention-look{width: 100%;height: 20px;line-height: 20px;margin-top: 40px;}
.sun-attention-look p{font-size:1.2rem;color: #222222;width: 200px;float: left;}
.sun-attention-look p span{margin-right: 10px;}
.sun-attention-look b{float: right;font-size:1.2rem;font-weight: normal;/*margin-top: 17px;*/}

/*****搜索*****/
.sun-search{width: 100%;margin-top: 10px;box-sizing: border-box;}
.sun-search ul li{width: 94%;border-bottom: 1px #e1e1e1 solid;background: #fff;padding: 15px 3%;}
.sun-search ul li .search-img{float: left;}
.sun-search ul li .search-img img{width: 100%;max-width:80px;height: 80px;vertical-align: middle;}
.sun-search ul li .search-div{float: left;width: 55%;margin-left: 15px;}
.sun-search.seach-noRight ul li .search-img{width: 20%;}
.sun-search.seach-noRight ul li .search-div{width:78%;margin-left: 0;padding-left: 2%;}
.sun-search ul li .search-div p{font-size:1.2rem;line-height: 24px;color: #222222;}
.sun-search ul li .search-div div{margin-top: 10px;}
.sun-search ul li .search-div div span{color: #ff5e3e;font-size:1.125rem;}
.sun-search ul li .search-div div i{color: #999999;font-size:1.125rem;text-decoration:line-through;padding-left: 10px;}
.sun-nosearch{width: 100%;text-align: center;margin-top: 40px;}
.sun-nosearch p{font-size:1.2rem;color: #222222;margin-top: 20px;}
.sun-nosearch a{background: #fbfafa;border: 1px #cbcbcb solid;text-align: center;display: block;width: 50%;margin: 0 auto;font-size: 1.25rem;color: #666666;padding: 8px 0;border-radius: 5px 5px;margin-top: 20px;}
/*******全部订单**********/
.sun-bale{width: 100%;}
.sun-bale-tab1{width: 94%;padding: 0 3%;}
.sun-bale-order{width: 100%;background: #fff;margin-top: 10px;}
.sun-bale-name{width: 94%;padding: 0 3%;border-bottom: 1px #f0f0f0 solid;font-size: 1rem;line-height: 26px;padding-top: 10px;display: flex;display: -webkit-box;display: -webkit-flex;}
.sun-bale-name div{font-size:1.2rem;-webkit-box-flex:3;flex:3;}
.sun-bale-name div p{color: #222;}
.sun-bale-name span{color: #ff5e3e;font-size:1.2rem;-webkit-box-flex:1;flex:1;text-align: right;}
.sun-bale-order1{width: 94%;padding: 0 3%;background: #fff;}
.sun-bale-order1 ul li{width: 100%;border-bottom: 1px #f0f0f0 solid;}
.sun-bale-order1 ul li:last-child{border: 0;}
.sun-order-img{width: 80px;height: 80px;}
.sun-order-img img{width: 80px;height: 80px;}
/*.sun-order-pro{margin-left: 2%;-webkit-box-flex:3;flex:3;}
.sun-order-pro p{font-size:1.125rem;color: #222222;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;word-break: break-all; }
.sun-order-pro span{color: #999999;font-size:0.95rem;padding:8px 5px 0 0;float: left;}*/
.sun-order-price{width:70px;text-align: right;}
.sun-order-price span{width: 100%;font-size: 1.125rem;color: #ff5e3e;float: left;}
.orderDetailsWrap .sun-order-price span{color:#222;}
.sun-order-price .sun-span1{color: #666666;}
/**退款状态印章**/
.orderDetailsWrap .sun-bale-order1 li{position: relative;}
.orderDetailsWrap .return-status{width: 90px;height: 72px;position: absolute;background: url(../images/returnStatus.png) no-repeat; right: 10px;top: 10px;background-size: 300% 100%;}
.orderDetailsWrap .return-status.returning{background-position:0 }
.orderDetailsWrap .return-status.returnSuccess{background-position:-90px;}
.orderDetailsWrap .return-status.returnFail{background-position:-180px;}


.sun-all{width: 94%;padding:0 3%;background: #f5f5f5;border-top: 1px #f0f0f0 solid;font-size: 1rem;}
.sun-all .sun-all-p1{color: #222222;float: left;font-size: 1rem;}
.sun-all .sun-all-p1 span{padding-right: 5px;}
.sun-all .sun-all-p2{float: right;}
.sun-handle{width: 100%;background: #fff;height:45px;text-align: right;line-height:45px;padding:10px 0;}
.sun-handle a{border: 1px #cbcbcb solid;color: #666666;font-size:1rem;text-align: center;padding:8px 1rem;border-radius: 5px 5px;margin-right: 3%;}
.sun-handle .sun-pay{background: #ff5e3e;color: #fff;border:1px #ff5e3e solid;}
/*****************物流查询*******************/
.Sun-logistics{width: 100%;}
.Sun-logistics-state{background: #fff;width: 94%;padding: 10px 3%;}
.Sun-logistics-state p{width: 100%;font-size:1.2rem;line-height:30px;color: #999999;}
.Sun-logistics-state .sun-state{color: #222222;font-size: 1.2rem;}
.Sun-logistics-state .sun-state span{color: #ff5e3e;}
.sun-logistics-liu{width: 94%;padding: 10px 3% 0;background: #fff;margin-top: 10px;border-bottom: 1px solid #f0f0f0;}
.sun-logistics-liu p{border-bottom: 1px #f0f0f0 solid;font-size: 1.125rem;color: #222222;padding-bottom: 10px;}
.Sun-logistics .returnStep{margin-bottom: 0;padding-bottom: 0;}
.Sun-logistics .returnStep .returnRe,.Sun-logistics .returnStep .returnRe>dt{color:#666;font-size: 1.125rem;}
.Sun-logistics .sun-logistics-liu .returnRe.latest,.Sun-logistics .returnStep .returnRe.latest>dt{color:#ff5e3e;}
.Sun-logistics .sun-logistics-liu .returnRe:before{width:9px;height:9px;border-radius: 50%;background:#e1e1e1;content:" ";position: absolute;left:-5px;top:2px;}
.Sun-logistics .sun-logistics-liu .returnRe.latest:before{width: 22px;height: 22px;background: url(../images/toto_03.png) no-repeat center center;content: " ";position: absolute;left: -12px;top: -2px;background-size: 100%;}
.Sun-logistics .returnStep .returnRe{padding-bottom:35px;}

/************订单详情*************/
.sun-orderdetails{width: 100%;}
.sun-details-state{width: 90%;background:#22292c;color: #fff;padding: 10px 0 10px 10%;line-height: 24px;}
.sun-details-state p{font-size: 1.2rem;}
.sun-details-state span{font-size: 1rem;color:#7a8285;}
.sun-details-states .sun-detailes{font-size:1rem;color: #222222;border-bottom: 1px #e1e1e1 solid;width: 94%;padding: 10px 3% 0 3%;background: #fff;}
.orderDetailsTop .sun-detailes,.orderDetailsTop .sun-install{border-bottom: none;}
.orderDetailsTop .sun-install>div{padding: 0 3%;border-bottom: none;}
.orderDetailsTop .sun-install>div div{height: 32px;line-height: 32px;border-bottom: 1px solid #f0f0f0;}
/*.sun-details-states div:first-child{padding-bottom: 10px;}*/
.sun-details-states .sun-detailes span{float: left;}
.sun-details-states .sun-detailes a{float: right;}
.sun-details-states .sun-detailes a img{width: 10px;height:100%;}
/*.sun-details-states .sun-detailes i{width: 70%;float: right;font-style: normal;}*/
.sun-details-states .sun-detailes p{color: #666666;}
.orderDetailsTop .sun-detailes p{margin-bottom: 10px;}
.sun-details-states .sun-detailes div{margin-top: 5px;/*float: left;*/padding: 5px 0;}
