.orderDetailsWrap .sun-all{border-bottom: 1px solid #f0f0f0;background: #f9f9f9;}
.orderDetailsWrap .sun-all>p{text-align: left;}
.orderDetailsWrap .addSunInfo{width: 100%;padding: 0 3%;margin: 10px -3% 0 -3%;border-bottom: none;}
.sun-detaile-time{width: 94%;padding: 10px 3%;color: #666666;background: #fff;border-top: 10px #f5f5f5 solid;font-size:1rem;line-height: 30px;border-bottom: 1px solid #f0f0f0;margin-bottom:50px;}
.sun-detailes-bottom{position: fixed;width: 100%;background: #fff;bottom: 0;display: flex;display: box; display: -webkit-box;display: -webkit-flex;flex-flow: row;}
.sun-detailes-bottom a{flex:1;-webkit-box-flex: 1;-webkit-flex: 1;height: 40px;line-height: 40px;font-size:1.2rem;text-align: center;border-right: 1px #f0f0f0 solid;border-top: 1px #f0f0f0 solid;display: block;}
.sun-detailes-bottom a:last-child{border-right: 0;}
/*******************我的预约*************/
.sun-mysever-one{line-height: 28px;}
.myreservationWrap .sun-bale-order{border-bottom: 1px solid #e1e1e1;}
.myreservationWrap .sun-bale-order1{width: 100%;padding: 0;}
.myreservationWrap .sun-bale-order1 ul li{padding:0;}
.myreservationWrap .sun-bale-order1 ul li>a{width: 94%;padding:10px 3%;}
.service-type{float: left;}
.service-status{text-align: right;color:#ff5e3e!important;}
.myreservationWrap .sun-bale-order1  .sun-mysever-cz{margin:0 0 15px 0}
.myreservationWrap .log-tips{border-bottom: none;padding: 0 3%;}
.myreservationWrap .log-tips span,.myreservationWrap .log-tips em{float: left;display: block;}
.myreservationWrap .log-tips em{float: right;}
.sun-mysever-cz{width: 100%;text-align: right;margin: 15px 0 5px 0;}
.sun-mysever-cz a{border: 1px #cbcbcb solid;border-radius: 5px 5px;text-align: center;font-size:1.2rem;color: #666666;padding: 4px 4%;margin-left: 5px;margin-right: 3%;}
