@charset "UTF-8";
/***预约***/
.sun-order{
	width: 100%;
	height: 100%;
}
.sun-order table{
	background: #fff;
	width: 100%;
	margin: 0 auto;
	box-sizing: border-box;
}
.sun-order table tr td{
	font-size:1.2rem;
	color: #222222;
	height: 50px;
	line-height: 50px;
	width: 50%;
	border-bottom: 1px #e1e1e1 solid;
	padding: 0 3%;
}
.sun-order table tr:nth-child(3){
	border-bottom: 10px #f5f5f5 solid;
}
.sun-order table tr:nth-child(5){
	border-bottom: 10px #f5f5f5 solid;
}
.sun-order table tr:nth-child(6){
	border-bottom: 10px #f5f5f5 solid;
}
/*.sun-order table tr:nth-child(7){
	border-bottom: 2px #f5f5f5 solid;
}*/
.sun-order table tr .sun-order-right{
	text-align: right;
	color: #999999;
}
.sun-order table tr .sun-order-right img{
	width: 10px;
	height: 20px;
	vertical-align: middle;
	padding-left: 10px;
}
.sun-order table tr .sun-order-right input{
	background: #fff;
	border: 0;
	height: 45px;
	line-height: 45px;
	font-size:1.2rem;
	color: #999999;
	text-align: right;
}
.sun-order table tr textarea{
	border: 0;
	width: 96%;
	height: auto;
	font-size:1.2rem;
	color: #999999;
	float: left;
	min-height: 70px;
}
.sun-order table tr span{
	width: 3px;
	height: 18px;
	background: #ff5e3e;
	display: block;
	float: left;
	margin-top: 7px;
}
.sun-order-bottom {
	width: 100%;
	margin-top: 20px;
}
.sun-order-bottom a{
	width: 90%;
	background: #ff5e3e;
	color: #fff;
	text-align: center;
	height: 40px;
	line-height: 40px;
	border-radius: 5px;
	display: block;
	font-size:1.2rem;
	margin: 0 auto;
}
/***微信关注***/
.sun-attention{
	width: 96%;
	height: 100%;
	padding:0 3% 20% 3%;
	background: #fff;
}
.sun-attention p{
	width: 100%;
	font-size:1.2rem;
	color: #222222;
}
.sun-attention-time{
	width: 100%;
	font-size:1rem;
	margin-top: 10px;
}
.sun-attention-time b{
	color: #2d74bd;
	padding-left: 15px;
	font-weight: normal;
}
.sun-attention-wei{
	width: 92%;
	border: 1px #ccc solid;
	margin-top: 20px;
	padding: 16px 4%;
	height: 120px;
	line-height: 120px;
	background: #f7f7f7;
}
.sun-attention-wei img{
	float: left;
	vertical-align: middle;
}
.sun-attention-wei span{
	float: left;
	font-size:1.2rem;
	color: #222222;
	padding-left: 15px;
}
.sun-attention-look{
	width: 100%;
	height: 20px;
	line-height: 20px;
	margin-top: 40px;
}
.sun-attention-look p{
	font-size:1.2rem;
	color: #222222;
	width: 200px;
	float: left;
}
.sun-attention-look p span{
	margin-right: 10px;
	}
.sun-attention-look b{
	float: right;
	font-size:1.2rem;
	font-weight: normal;
   /*margin-top: 17px;*/
}

/*****搜索*****/
.sun-search{
	width: 100%;
	margin-top: 10px;
	box-sizing: border-box;
}
.sun-search ul li{
	width: 94%;
	border-bottom: 1px #e1e1e1 solid;
	background: #fff;
	padding: 15px 3%;
}
.sun-search ul li .search-img{
	float: left;

}
.sun-search ul li .search-img img{
	width: 100%;
	max-width:80px;
	height: 80px;
	vertical-align: middle;
}
.sun-search ul li .search-div{
	float: left;
	width: 55%;
	margin-left: 15px;
}
.sun-search ul li .search-div p{
	font-size:1.2rem;
	line-height: 24px;
	color: #222222;
}
.sun-search ul li .search-div div{
	margin-top: 10px;
}
.sun-search ul li .search-div div span{
	color: #ff5e3e;
	font-size:1.125rem;
}
.sun-search ul li .search-div div i{
	color: #999999;
	font-size:1.125rem;
	text-decoration:line-through;
	padding-left: 10px;
}
.sun-nosearch{
	width: 100%;
	text-align: center;
	margin-top: 40px;
}
.sun-nosearch p{
	font-size:1.2rem;
	color: #222222;
	margin-top: 20px;
}
.sun-nosearch a{
	background: #fbfafa;
	border: 1px #cbcbcb solid;
	text-align: center;
	display: block;
	width: 50%;
	margin: 0 auto;
	font-size: 1.25rem;
	color: #666666;
	padding: 8px 0;
	border-radius: 5px 5px;
	margin-top: 20px;
}
