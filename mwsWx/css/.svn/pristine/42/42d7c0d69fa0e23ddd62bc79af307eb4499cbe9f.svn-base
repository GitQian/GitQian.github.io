.choosePage .chooseInfo img{width: 15%;max-width: 100px;}
.choosePage .chooseInfo span{font-size:1.2rem;padding-left:20px;vertical-align: top;margin-top: 20px;display: inline-block;}
.choosePage .chooseleft{position: fixed;right: 0px;top:45px;}
.choosePage .chooseleft ul{height:100%;display: flex;display:-webkit-box;display:-webkit-flex;flex-flow: column;-webkit-flex-flow:column;-webkit-box-orient:vertical;}
.choosePage .chooseleft ul li{flex:1;-webkit-box-flex: 1;-webkit-flex: 1;}
.choosePage .chooseleft a{width: 40px;display: block;text-align: center;font-size:0.95rem;}


/**商品详情**/
.product-wrap{background:#ebebeb}
.product-wrap .swipe-wrap>div{height: 330px;line-height: 330px;text-align: center;}
.product-wrap .swipe-wrap>div img{max-height: 330px;width:auto;vertical-align: middle;}
.product-wrap .sun-banner{background: #fff;padding-bottom: 10px;}
.product-wrap .sun-banner .pro-info{padding: 0 3%;width: 94%;}
.product-wrap .sun-banner .pro-info h2{font-size: 1.125rem;margin:10px 0;}
.product-wrap .pro-info p{margin-bottom: 10px;}
.product-wrap .pro-info .pro-price{color:#ff5e3e;font-size: 1.2rem;}
.product-wrap .pro-price .old-price{color: #999;text-decoration:line-through;padding-left: 10px;}
.product-wrap .pro-delivery span{display:block;float: left;color:#999;font-size:0.95rem;}
.product-wrap .pro-delivery span:last-child{float: right;}
.product-wrap .offShelf{font-size: 1.5rem;text-align: center;line-height: 3rem;background: #ccc;margin: 0 -3%;}
.iden-wrap{line-height:3rem;font-size:0.95rem;padding: 0 3%;width: 94%;}
.iden-wrap{background:#f9f9f9;}
.iden-wrap span{padding-right:10px ;}
.iden-wrap span .iden-icon{border:none;width:20px;height: 20px;margin-right:3px;vertical-align:-5px ;display: inline-block;background:url(../images/icon_choose.png) 0 0 no-repeat;background-size:200% 100%;}
.iden-wrap span .iden-icon.selected{background-position: -20px;}
.type-btn{width: 94%;padding:10px 3%;line-height:2.2rem;font-size: 1.125rem;margin:10px 0;background:#fff url(../images/icon_zjt.png) 97% 50% no-repeat;background-size:3%;}
.product-wrap .sun-last{margin-bottom: 50px;position: relative;}
.operate-wrap{z-index: 100}
.operate-wrap span{text-align: center;border-right: 1px solid #c0c0c0;display: block;flex:1;-webkit-box-flex: 1;-webkit-flex: 1;}
.operate-wrap span img{width: 100%;height: auto;max-width: 30px;vertical-align: middle;}
.operate-wrap .opt-cart,.operate-wrap .opt-shopping{flex:2;-webkit-box-flex: 2;-webkit-flex: 2;}

/**退换货**/
.return-order.sun-shoppingCart .sun-order-price{width: 28%;}
.order-return-title{line-height:3rem;background: #fff;font-size: 1.125rem;border-bottom: 1px solid #e1e1e1;}
.order-return-title span{padding-left: 3%;}
.return-order .iden-wrap,.add-message,.return-info,.add-pic{background: #fff;margin-top: 10px;} 
.add-message,.add-pic{padding:10px 3%;border-bottom: 1px solid #e1e1e1;font-size: 1.125rem;} 
.add-message span{border-left: 3px solid #ff5e3e;padding-left: 5px;vertical-align: top;font-size: 1.125rem;}
.add-message textarea{width: 64%;border:none;min-height: 100px;font-size:1.125rem;padding: 0;border-radius: 0;}
.return-info dl{line-height:3rem;border-bottom: 1px solid #e1e1e1;padding:0 3%;font-size: 1.125rem;}
.return-info dl dt,dl dd{float: left;}
.return-info dl dt{display: inline-block;width:30%;}
.return-info dl dd{width:70%;}
.return-info dl dd input{width:100%;line-height: 2.2rem;border: none;font-size:1.125rem;}
.add-pic p em{color:#ccc;}
.add-pic .pic-box{height: 5rem;margin:15px 0 0;}
.add-pic .pic-box .upload-pic{position: relative;cursor: pointer;width: 5rem;height: 5rem;overflow: hidden;float:left;display: block;background: url(../images/face.png) no-repeat;background-size: 100%;margin-right:10px;}
.add-pic .pic-box .upload-pic input{border-radius: 0;width: 100%;height: 100%;position: absolute;font-size: 100px;right: 0;top: 0;opacity: 0;filter: alpha(opacity=0);cursor: pointer;}
.return-order .btn-box{margin-top: 50px;}

