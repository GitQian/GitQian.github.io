.pcPage .edit-img-toux .face-edit{ position: relative;cursor: pointer;width:5rem;height: 5rem;overflow: hidden;float:right;*display: inline;*zoom: 1;display: block;background: url(../images/face.png) no-repeat;background-size: 100%; }
