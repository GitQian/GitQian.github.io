@charset "UTF-8";

html,body,h1,h2,h3,h4,h5,h6,p,br,form,input,button,textarea,select,fieldset,legend,blockquote,ul,ol,li,dl,dt,dd,pre,th,td,div{margin:0;padding:0;}
html{font-size:100%;}
body{font-family: 'Microsoft YaHei';color:#222;background: #f5f5f5;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
a{color: #333;text-decoration: none;cursor: pointer;outline:none;}
a:hover{text-decoration:none;outline:none;}
a:active {star:expression(this.onFocus=this.blur());}
a:focus{outline:none;-moz-outline:none;}
textarea,select,input,button{vertical-align:middle;font:12px Tahoma;resize: none;}
textarea{padding: 5px;border-radius: 5px;border:1px solid #E0E0E0;}
button{background:none;border:none;cursor:pointer}
address,cite,code,em,var,acronym{font-style:normal;}
pre{font-size:14px;text-align:center;white-space:pre-wrap;}
table{border-collapse:collapse;}
div,li,dt,dd,p,pre,td,th{word-wrap:break-word;word-break:break-all;}
ol,ul{list-style:none;}
li{list-style-position:outside;}
input{outline:none;}
label,input[type="radio"]{cursor: pointer;}
img {border: 0;max-width: 100%;height: auto; }
fl{ float: left;}
fl{ float: right;}
input,textarea{-webkit-appearance:none;outline:none;}
a,img,button,input,textarea{-webkit-tap-highlight-color:rgba(255,255,255,0);}

/*clear float*/
.clearfix:before,.clearfix:after{content:".";display:block;height:0;visibility:hidden;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.clearall{overflow:hidden;_zoom:1;}
.clear{clear:both;font-size:0;height:0;line-height:0;}

/*公共*/
.mt15{margin-top: 15px;}
.mt50{margin-top:50px;}

.wxPage .pagebox{background: #fff;width: 100%;border-bottom: 1px solid #e1e1e1;}
.wxPage .pagebox .page-box{padding: 0 2%;margin: 0 auto;}

/*线下门店*/
