.pcPage .pc-edit-info input::-webkit-input-placeholder {text-align: right;}
.pcPage .pc-edit-info input::-moz-placeholder {text-align: right;} /* firefox 19+ */
.pcPage .pc-edit-info input::-ms-input-placeholder {text-align: right;} /* ie */
.pcPage .pc-edit-info input:-moz-placeholder {text-align: right;}
/*编辑资料-下拉单*/
.ui-select {text-align: right;position: relative;padding-right:10%;overflow: hidden; }  
.ui-select select {position: absolute;left: 0px;top:-1.8rem;width: 100%;height:3em;opacity: 0;} 
